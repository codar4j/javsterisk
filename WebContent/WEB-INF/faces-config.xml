<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

<navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>HomeList</from-outcome>
   <to-view-id>/main/HomeList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>DashboardList</from-outcome>
   <to-view-id>/main/DashboardList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>UserList</from-outcome>
   <to-view-id>/main/UserList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ExtensionsList</from-outcome>
   <to-view-id>/main/ExtensionsList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>SipBuddiesList</from-outcome>
   <to-view-id>/main/SipBuddiesList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>LoggingEventList</from-outcome>
   <to-view-id>/main/LoggingEventList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>LogAsterisk</from-outcome>
   <to-view-id>/main/LogAsteriskList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>PreferenceList</from-outcome>
   <to-view-id>/main/PreferenceList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ParameterList</from-outcome>
   <to-view-id>/main/ParameterList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>PeerList</from-outcome>
   <to-view-id>/main/PeerList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ChannelList</from-outcome>
   <to-view-id>/main/ChannelList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>SystemList</from-outcome>
   <to-view-id>/main/SystemList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>CdrList</from-outcome>
   <to-view-id>/main/CdrList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>CliAsterisk</from-outcome>
   <to-view-id>/main/CliAsteriskList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>es</supported-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
  <resource-bundle>
   <base-name>messages</base-name>
   <var>msg</var>
  </resource-bundle>
 </application>
 <factory>
  <exception-handler-factory>org.primefaces.extensions.component.ajaxerrorhandler.AjaxExceptionHandlerFactory</exception-handler-factory>
 </factory>

</faces-config>
